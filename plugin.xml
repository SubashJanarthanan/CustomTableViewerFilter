         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            name="CustomTableViewerFilter"
            application="org.eclipse.e4.ui.workbench.swt.E4Application">
         <property
               name="lifeCycleURI"
               value="bundleclass://CustomTableViewerFilter/customtableviewerfilter.E4LifeCycle">
         </property>
